<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/product_details_layout"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/gradient_layout_product_detail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbar_layout"
        android:layout_width="match_parent"
        android:layout_height="256dp"
        android:fitsSystemWindows="true"
        app:elevation="0dp"

        android:theme="@style/Theme.AnJuConsumerApp">

        <com.google.android.material.appbar.CollapsingToolbarLayout
            android:id="@+id/collapsing_toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fitsSystemWindows="true"
            app:expandedTitleTextAppearance="@style/MyExpandedTitle"
            app:collapsedTitleTextAppearance="@style/MyCollapsedTitle"
            app:collapsedTitleTextColor="@color/black"
            app:collapsedTitleGravity="bottom"
            android:background="@drawable/gradient_layout_product_detail"
            app:layout_scrollFlags="scroll|snap|exitUntilCollapsed">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivProductImage"
                android:layout_width="256dp"
                android:layout_height="256dp"
                android:layout_gravity="center"
                android:scaleType="centerCrop"
                android:src="@drawable/carrot__1_" />

            <com.google.android.material.appbar.MaterialToolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:visibility="gone"
                android:layout_height="?attr/actionBarSize"
                app:layout_collapseMode="parallax"
                />
        </com.google.android.material.appbar.CollapsingToolbarLayout>
    </com.google.android.material.appbar.AppBarLayout>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="com.google.android.material.appbar.AppBarLayout$ScrollingViewBehavior"
        android:background="@drawable/curved_layout_product_details"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:animateLayoutChanges="true"
            >

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_ProductName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="24dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:text="Tomato"
                android:textSize="28sp"
                android:textColor="@color/primary_text_color"
                android:fontFamily="@font/poppins_semibold"
                />
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.15"
                />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_Expiry"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="28dp"
                android:layout_marginEnd="16dp"
                android:textSize="16sp"
                android:fontFamily="@font/poppins_bold"
                android:textColor="@color/primary_text_color"
                android:text="@string/expire_date"
                />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_ExpiryDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/tv_Expiry"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="16dp"
                android:text="16 May 2023"
                android:fontFamily="@font/poppins_medium"
                android:textColor="@color/TextCopyBackground"
                />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/description_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                android:fontFamily="@font/poppins_bold"
                android:text="@string/description"
                android:textColor="@color/primary_text_color"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_ExpiryDate" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_description"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/description_text"

                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:gravity="fill"
                android:textColor="@color/primary_text_color"
                android:fontFamily="@font/poppins"
                android:text="Hello this is a product and this is a description and how are you are you "
                />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/prdct_details"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_description"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:textSize="16sp"
                android:textColor="@color/primary_text_color"
                android:fontFamily="@font/poppins_bold"
                android:text="@string/product_details"
                />
            <RelativeLayout
                android:id="@+id/rl_product_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/prdct_details"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                >
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/productId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins_medium"
                    android:text="Product ID :"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/productIdValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@id/productId"
                    android:layout_marginStart="12dp"
                    android:text="123456789"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/blueColor"
                    />
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/copyProductId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@id/productIdValue"
                    android:layout_marginStart="12dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/baseline_content_copy_24"
                    android:tint="@color/primary_text_color"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_seeding_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/rl_product_id"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                >
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_product_details"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins_medium"
                    android:text="@string/seeding_date"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_seeding_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@id/tv_product_details"
                    android:layout_marginStart="12dp"
                    android:text="16 May 2023"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/blueColor"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/rl_seeding_date"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                >
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins_medium"
                    android:text="@string/price_text"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_price_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@id/tv_price"
                    android:layout_marginStart="12dp"
                    android:text="₹ 100"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/blueColor"
                    />
            </RelativeLayout>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_product_journey_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/rl_price"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_margin="16dp"
                android:text="@string/product_journey"
                android:textColor="@color/primary_text_color"
                android:fontFamily="@font/poppins_bold"
                android:textSize="16sp"
                />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/rl_pj_itmlstd"
                app:layout_constraintTop_toBottomOf="@id/tv_product_journey_text"
                android:layout_margin="20dp"
                >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/iv_product_added"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/cart"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_product_list"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@id/iv_product_added"
                    android:layout_marginStart="12dp"
                    android:textSize="14sp"
                    android:fontFamily="@font/poppins_medium"
                    android:textColor="@color/blueColor"
                    android:text="@string/product_listed"
                    />
                <TextView
                    android:id="@+id/tv_product_added_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_product_list"
                    android:layout_toEndOf="@id/iv_product_added"
                    android:layout_marginStart="12sp"
                    android:text="12 May 2021"
                    android:textSize="12sp"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_product_added_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="12:00"
                    android:layout_below="@id/tv_product_list"
                    android:layout_marginStart="8dp"
                    android:layout_toEndOf="@id/tv_product_added_date"
                    android:textSize="12sp"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
            </RelativeLayout>
            <View
                android:id="@+id/view_pj_itmlstd"
                android:layout_width="2dp"
                android:layout_height="48dp"
                android:background="@color/blueColor"
                app:layout_constraintTop_toBottomOf="@id/rl_pj_itmlstd"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="44dp"
                />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/rl_order_details"
                android:visibility="visible"
                app:layout_constraintTop_toBottomOf="@id/view_pj_itmlstd"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"

                >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/iv_order_preview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/bag"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_order_preview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@id/iv_order_preview"
                    android:layout_marginStart="12dp"
                    android:textSize="14sp"
                    android:fontFamily="@font/poppins_medium"
                    android:textColor="@color/blueColor"
                    android:text="Order details"
                    />
                <TextView
                    android:id="@+id/tv_order_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_order_preview"
                    android:layout_toEndOf="@id/iv_order_preview"
                    android:layout_marginStart="24sp"
                    android:layout_marginTop="12dp"
                    android:textSize="12sp"
                    android:text="Order id :"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_order_id_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="123456789"
                    android:layout_below="@id/tv_order_preview"
                    android:layout_marginStart="8dp"
                    app:fontFamily="@font/poppins_medium"
                    android:layout_toEndOf="@id/tv_order_id"
                    android:layout_marginTop="12dp"
                    android:textSize="12sp"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_ordered_by"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Ordered by :"
                    android:layout_marginTop="4dp"
                    android:layout_below="@id/tv_order_id"
                    android:layout_toEndOf="@id/iv_order_preview"
                    android:layout_marginStart="24sp"
                    android:textSize="12sp"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_ordered_by_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rohit Kumar"
                    android:layout_below="@id/tv_order_id"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:layout_toEndOf="@id/tv_ordered_by"
                    android:textSize="12sp"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_order_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Order date :"
                    android:layout_marginTop="4dp"
                    android:layout_below="@id/tv_ordered_by"
                    android:layout_toEndOf="@id/iv_order_preview"
                    android:layout_marginStart="24sp"
                    android:textSize="12sp"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_ordered_date_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="12/12/2020"
                    android:layout_below="@id/tv_ordered_by"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:layout_toEndOf="@id/tv_order_date"
                    android:textSize="12sp"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_delivery_location"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/shipment_location"
                    android:layout_marginTop="4dp"
                    android:layout_below="@id/tv_order_date"
                    android:layout_toEndOf="@id/iv_order_preview"
                    android:layout_marginStart="24sp"
                    android:textSize="12sp"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_delivery_location_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Delhi"
                    android:layout_below="@id/tv_order_date"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:layout_toEndOf="@id/tv_delivery_location"
                    android:textSize="12sp"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_payment_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/payment_status"
                    android:layout_marginTop="4dp"
                    android:layout_below="@id/tv_delivery_location"
                    android:layout_toEndOf="@id/iv_order_preview"
                    android:layout_marginStart="24sp"
                    android:textSize="12sp"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_payment_status_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/in_transit"
                    android:layout_below="@id/tv_delivery_location"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:layout_toEndOf="@id/tv_payment_status"
                    android:textSize="12sp"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />

            </RelativeLayout>

            <View
                android:id="@+id/viewLine2"
                android:layout_width="2dp"
                android:layout_height="48dp"
                android:background="@color/blueColor"
                app:layout_constraintTop_toBottomOf="@id/rl_order_details"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="44dp"
                />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                android:visibility="gone"
                android:id="@+id/rl_facility_info"
                app:layout_constraintTop_toBottomOf="@id/viewLine2"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"

                >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/iv_facility_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/bag"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_facility_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@id/iv_facility_info"
                    android:layout_marginStart="12dp"
                    android:textSize="14sp"
                    android:fontFamily="@font/poppins_medium"
                    android:textColor="@color/blueColor"
                    android:text="@string/status"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/harvestingDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_facility_info"
                    android:layout_toEndOf="@id/iv_facility_info"
                    android:layout_marginStart="24sp"
                    android:layout_marginTop="12dp"
                    android:textSize="12sp"
                    android:text="Harvesting Date :"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/harvestDateValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_facility_info"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="12dp"
                    android:layout_toEndOf="@id/harvestingDate"
                    android:textSize="12sp"
                    android:text="20 May 2023"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/processingDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/harvestingDate"
                    android:layout_toEndOf="@id/iv_facility_info"
                    android:layout_marginStart="24sp"
                    android:layout_marginTop="4dp"
                    android:textSize="12sp"
                    android:text="Processing Date :"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/processingDateValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/harvestDateValue"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:layout_toEndOf="@id/processingDate"
                    android:textSize="12sp"
                    android:text="20 May 2023"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/packagingDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/processingDate"
                    android:layout_toEndOf="@id/iv_facility_info"
                    android:layout_marginStart="24sp"
                    android:layout_marginTop="4dp"
                    android:textSize="12sp"
                    android:text="Packaging Date :"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/packagingDateValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/processingDateValue"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:layout_toEndOf="@id/packagingDate"
                    android:textSize="12sp"
                    android:text="20 May 2023"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/packaginingFacility"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/packagingDate"
                    android:layout_toEndOf="@id/iv_facility_info"
                    android:layout_marginStart="24sp"
                    android:layout_marginTop="4dp"
                    android:textSize="12sp"
                    android:text="Packaging Facility :"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/packaginingFacilityValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/packagingDateValue"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:layout_toEndOf="@id/packaginingFacility"
                    android:textSize="12sp"
                    android:text="20 May 2023"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/order_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/packaginingFacility"
                    android:layout_toEndOf="@id/iv_facility_info"
                    android:layout_marginStart="24sp"
                    android:layout_marginTop="4dp"
                    android:textSize="12sp"
                    android:text="Origin :"
                    android:textColor="@color/yellowish"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/order_status_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/packaginingFacilityValue"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:layout_toEndOf="@id/order_status"
                    android:textSize="12sp"
                    android:text="Shipped..."
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/yellowish"
                    />


            </RelativeLayout>

            <View
                android:id="@+id/viewLine3"
                android:layout_width="2dp"
                android:layout_height="48dp"
                android:background="@color/blueColor"
                app:layout_constraintTop_toBottomOf="@id/rl_facility_info"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="44dp"
                />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/rl_delivery_location_status"
                app:layout_constraintTop_toBottomOf="@id/viewLine3"
                android:layout_margin="20dp"

                >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/iv_delivery_location_status"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:src="@drawable/baseline_location_on_24"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_location_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@id/iv_delivery_location_status"
                    android:layout_marginStart="12dp"
                    android:textSize="14sp"
                    android:fontFamily="@font/poppins_medium"
                    android:textColor="@color/blueColor"
                    android:text="Product Movement Details :"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/order_stats"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_location_text"
                    android:layout_toEndOf="@id/iv_delivery_location_status"
                    android:layout_marginStart="24sp"
                    android:layout_marginTop="12dp"
                    android:textSize="12sp"
                    android:text="Order Status :"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/order_stats_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_location_text"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="12dp"
                    android:layout_toEndOf="@id/order_stats"
                    android:textSize="12sp"
                    android:text="Moving..."
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/location_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/order_stats"
                    android:layout_toEndOf="@id/iv_delivery_location_status"
                    android:layout_marginStart="24sp"
                    android:layout_marginTop="4dp"
                    android:textSize="12sp"
                    android:text="Location Reached :"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/location_status_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/order_stats_value"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:layout_toEndOf="@id/location_status"
                    android:textSize="12sp"
                    android:text="Delhi"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/delivery_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/location_status"
                    android:layout_toEndOf="@id/iv_delivery_location_status"
                    android:layout_marginStart="24sp"
                    android:layout_marginTop="4dp"
                    android:textSize="12sp"
                    android:text="Delivery Date :"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/deliver_date_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/location_status_value"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:layout_toEndOf="@id/delivery_date"
                    android:textSize="12sp"
                    android:text="21 May 2023"

                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/delivery_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/delivery_date"
                    android:layout_toEndOf="@id/iv_delivery_location_status"
                    android:layout_marginStart="24sp"
                    android:layout_marginTop="4dp"
                    android:textSize="12sp"
                    android:text="Delivery Time :"
                    android:textColor="@color/primary_text_color"
                    android:fontFamily="@font/poppins"
                    />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/delivery_time_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/deliver_date_value"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:layout_toEndOf="@id/delivery_time"
                    android:textSize="12sp"
                    android:text="12:00 PM"
                    android:fontFamily="@font/poppins"
                    android:textColor="@color/primary_text_color"
                    />
            </RelativeLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>
</androidx.coordinatorlayout.widget.CoordinatorLayout>